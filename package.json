{
  "name": "react-tabllist",
  "version": "1.6.0",
  "description": "React-based list (table) components that support events, callback functions, and custom styles.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config build/webpack.config.dev.js",
    "test": "jest --coverage --detectOpenHandles --forceExit && codecov",
    "tjest": "jest --detectOpenHandles --forceExit",
    "clean-demo": "rm -rf examples/",
    "clean-dist": "rm -rf dist/",
    "start-demo": "webpack-dev-server --config build/webpack.config.dev.demo",
    "demo": "yarn clean-demo && webpack --config build/webpack.config.prod.demo",
    "push-demo": "git subtree push -P examples github gh-pages && git subtree push -P examples coding coding-pages",
    "push": "git push github master && git push coding master",
    "build": "yarn clean-dist && webpack --config build/webpack.config.prod",
    "lint": "stylelint-config-prettier-check"
  },
  "keywords": [
    "react",
    "table",
    "list"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:oceanxy/react-tabllist.git"
  },
